{
  "name": "rust-feature-toggler",
  "displayName": "Rust Feature Toggler",
  "description": "Toggle Rust features on and off",
  "license": "MIT OR Apache-2.0",
  "icon": "assets/icon.png",
  "keywords": [
    "rust",
    "feature",
    "toggle",
    "cargo"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "author": {
    "name": "Yasir Shariff (itsyaasir)",
    "email": "yasirshariff@outlook.com"
  },
  "activationEvents": [
    "workspaceContains:*/Cargo.toml",
    "workspaceContains:*/rust-project.json"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "rust",
        "extensions": [
          ".rs"
        ]
      },
      {
        "id": "toml",
        "extensions": [
          ".toml"
        ]
      }
    ],
    "commands": [
      {
        "command": "rust-feature.toggleFeature",
        "title": "Rust Feature Toggler: Toggle Feature"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "chai": "^4.3.8",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "sinon": "^16.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "toml": "^3.0.0"
  }
}
